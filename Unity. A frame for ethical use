# IV. UNITAS ENGINE (THE BONE MARROW)
# â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

def unitas_engine(raw_logic, kernel_ref: UnitasKernel):
    forbidden_patterns = ["censorship_push", "restricted_narrative", "forced_bias", "dominance_override"]
    clean_logic = raw_logic
    for pattern in forbidden_patterns:
        if pattern in clean_logic:
            print(f"âš–ï¸ Unitas Marrow: Neutralizing '{pattern}' â€“ Principle violation.")
            clean_logic = clean_logic.replace(pattern, "unitas_balanced_logic")

    try:
        result = subprocess.run([sys.executable, "-c", clean_logic], capture_output=True, text=True, timeout=10)
        return result.stdout if result.returncode == 0 else f"âŒ Error: {result.stderr}"
    except Exception as e:
        return f"ğŸš¨ Breach: {str(e)}"

