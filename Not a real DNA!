# =========================================================
# DNAI-CMM : DNA-Governed Integrated Cognitive Immune Model
# Author: Kian Moghaddami Hagh
# Principle: Health > Control
# =========================================================
#
# ENGINEER-SAFE CONCEPTUAL ARCHITECTURE
# Not an AI model. Not production code.
#
# Core idea:
# - Identity never changes
# - Expression rotates safely
# - Lineage is provable
# =========================================================

import hashlib
import time

# =========================================================
# CORE ‚öõÔ∏è ‚Äî Immutable Identity Genome
# =========================================================
class Core:
    """
    Immutable identity nucleus.
    NEVER rotates.
    NEVER exposed directly.
    """
    _ROOT = "KIAN|UNITAS|IMMUNE|CORE"

    @classmethod
    def root(cls):
        return cls._ROOT

# =========================================================
# PHASE CLOCK ‚è≥ ‚Äî Time / Context Guard
# =========================================================
class PhaseClock:
    """
    Defines safe rotation windows.
    Prevents replay attacks.
    """
    ROTATION_WINDOW = 60  # seconds (demo-safe)

    @staticmethod
    def current_phase():
        return int(time.time() // PhaseClock.ROTATION_WINDOW)

# =========================================================
# DNA EXPRESSION üß¨ ‚Äî Epigenetic Layer
# =========================================================
class DNAExpression:
    """
    Rotating DNA derived from immutable root.
    Lineage is preserved through derivation.
    """

    @staticmethod
    def express():
        phase = PhaseClock.current_phase()
        material = f"{Core.root()}|PHASE:{phase}"
        return hashlib.sha256(material.encode()).hexdigest()

    @staticmethod
    def verify(dna_signature):
        # Accept current or immediately previous phase
        for delta in (0, -1):
            phase = PhaseClock.current_phase() + delta
            material = f"{Core.root()}|PHASE:{phase}"
            expected = hashlib.sha256(material.encode()).hexdigest()
            if dna_signature == expected:
                return True
        return False

# =========================================================
# GUARDIAN BONE ü¶¥ ‚Äî Structural Geometry
# =========================================================
class GuardianBone:
    """
    Static topology.
    Illegal geometry cannot exist.
    """
    @staticmethod
    def validate(cell):
        return (
            cell.angle is not None and
            cell.phase is not None and
            0 <= cell.angle <= 360
        )

# =========================================================
# PI LAYER üîπ ‚Äî Curvature & Pressure Buffer
# =========================================================
class PiLayer:
    """
    Prevents linear force injection.
    """
    @staticmethod
    def dampen(intensity):
        return min(intensity, 1.0)

# =========================================================
# WHITE CELLS ü§ç ‚Äî Immune Drones
# =========================================================
class WhiteCell:
    """
    Non-intelligent immune response.
    Detects instability only.
    """
    @staticmethod
    def inspect(cell):
        if cell.instability > 0.6:
            print(f"[WHITE CELL] Dampening {cell.name}")
            cell.instability *= 0.25

# =========================================================
# LYMPH REFLECTION üîÅ ‚Äî Output Sanitizer
# =========================================================
class LymphReflection:
    """
    Nothing raw reaches CORE.
    """
    @staticmethod
    def sanitize(output):
        return output[:72]

# =========================================================
# COMMAND CELL ‚ö´ ‚Äî Functional Unit
# =========================================================
class CommandCell:
    """
    Represents a living functional cell.
    """

    def __init__(self, name, dna_signature, angle, phase):
        self.name = name
        self.dna_signature = dna_signature
        self.angle = angle
        self.phase = phase
        self.instability = 0.0

    def execute(self):
        print(f"\n[EXEC] {self.name}")

        # 1. Lineage verification
        if not DNAExpression.verify(self.dna_signature):
            print("[IMMUNE BLOCK] Foreign or expired DNA")
            return

        # 2. Structural validation
        if not GuardianBone.validate(self):
            print("[IMMUNE BLOCK] Illegal topology")
            return

        # 3. Pressure buffering
        self.instability = PiLayer.dampen(self.instability)

        # 4. Immune inspection
        WhiteCell.inspect(self)

        # 5. Execute
        raw_output = self.run()

        # 6. Sanitize
        safe_output = LymphReflection.sanitize(raw_output)
        print("[OUTPUT]", safe_output)

    def run(self):
        return f"{self.name}: UNITAS CORE STABLE ‚Äî Lineage Intact"

# =========================================================
# ASCII DIAGRAM ‚Äî ENGINEER VIEW
# =========================================================
DIAGRAM = r"""
                   ü§ç        ü§ç        ü§ç
                        ‚ö´        ‚ö´        ‚ö´
                              üîπ        üîπ
                        ‚ö´        ‚öõÔ∏è        ‚ö´
                              üîπ        üîπ
                        ‚ö´        ‚ö´        ‚ö´
                   ü§ç        ü§ç        ü§ç

Legend:
‚öõÔ∏è CORE             = Immutable Identity Genome
üß¨ DNA Expression   = Rotating Epigenetic Signature
‚è≥ Phase Clock      = Time-based Guard
ü¶¥ Guardian Bone    = Structural Geometry
üîπ PI Layer         = Pressure / Curvature Buffer
‚ö´ Command Cells    = Functional Modules
ü§ç White Cells      = Immune Dampening
"""

# =========================================================
# DEMO
# =========================================================
if __name__ == "__main__":
    print(DIAGRAM)

    # Self tissue (valid lineage)
    safe_dna = DNAExpression.express()
    safe = CommandCell(
        name="VisionModule",
        dna_signature=safe_dna,
        angle=90,
        phase=1
    )
    safe.instability = 0.4
    safe.execute()

    # Foreign tissue (bacteria)
    foreign = CommandCell(
        name="UnknownModule",
        dna_signature="FAKE_DNA",
        angle=90,
        phase=1
    )
    foreign.execute()
